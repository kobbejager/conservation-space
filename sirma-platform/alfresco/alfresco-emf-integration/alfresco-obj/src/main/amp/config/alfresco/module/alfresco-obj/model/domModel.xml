<?xml version="1.0" encoding="UTF-8"?>

<model name="dom:cmfmodel" xmlns="http://www.alfresco.org/model/dictionary/1.0">

	<description>CMF Object Model</description>
	<author>Sirma ITT</author>
	<version>1.0</version>

	<imports>
		<import uri="http://www.alfresco.org/model/dictionary/1.0" prefix="d" />
		<import uri="http://www.alfresco.org/model/system/1.0" prefix="sys" />
		<import uri="http://www.alfresco.org/model/content/1.0" prefix="cm" />
		<import uri="http://www.alfresco.org/model/site/1.0" prefix="st" />
		<import uri="http://www.sirmaitt.com/model/emf/1.0" prefix="emf" />
		<import uri="http://www.sirmaitt.com/model/cmf/1.0" prefix="cmf" />
	</imports>

	<namespaces>
		<namespace uri="http://www.sirmaitt.com/model/dom/1.0" prefix="dom" />
	</namespaces>
	<constraints>

	</constraints>

	<types>

		<type name="dom:objectspace">
			<title>Object Container</title>
			<parent>cm:folder</parent>
			<mandatory-aspects>
				<aspect>dom:object</aspect>
			</mandatory-aspects>
		</type>
		<type name="dom:objectdefinitionspace">
			<title>Object Definitions Space</title>
			<parent>cm:folder</parent>
		</type>
		<type name="dom:objectinstancesspace">
			<title>Object Instances Space</title>
			<parent>cm:folder</parent>
		</type>


	</types>
	<!-- Aspects -->
	<aspects>
		<aspect name="dom:object">
			<title>Aspect for Object</title>
		</aspect>
		<aspect name="dom:objectDefinition">
			<title>Definition for Object</title>
		</aspect>
		<aspect name="dom:objectview">
			<title>View for object</title>
		</aspect>
	</aspects>


</model>