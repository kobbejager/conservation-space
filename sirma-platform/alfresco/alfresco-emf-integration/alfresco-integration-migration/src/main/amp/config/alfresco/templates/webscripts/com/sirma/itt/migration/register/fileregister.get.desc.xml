<webscript>
	<shortname>File register - search in the register</shortname>
	<description>
		Web script for searching into the register
	</description>
	<url>/fileregister/{mode}</url>
	<url>/fileregister/{mode}/{crcCode}</url>
	<url>/fileregister/{mode}?pageSize={pageSize?}&amp;skip={skip?}&amp;total={total?}&amp;source={source?}&amp;name={name?}&amp;modifiedBy={modifiedBy?}&amp;modifiedFrom={modifiedFrom?}&amp;modifiedTo={modifiedTo?}&amp;status={status?}&amp;target={target?}</url>
	<authentication>user</authentication>
	<transaction allow="readonly">required</transaction>
	<format default="json" />
	<negotiate accept="application/json">json</negotiate>
</webscript>
