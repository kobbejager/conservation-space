<?xml version="1.0" encoding="UTF-8"?>
<mappings xmlns="http://dozer.sourceforge.net" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://dozer.sourceforge.net
          http://dozer.sourceforge.net/schema/beanmapping.xsd">

	<mapping>
		<class-a>com.sirma.itt.seip.model.InstanceEntity</class-a>
		<class-b>com.sirma.itt.seip.instance.CommonInstance</class-b>

		<field-exclude>
			<a>operation</a>
			<b>operation</b>
		</field-exclude>
		<field-exclude>
			<a>fieldHandler</a>
			<b>fieldHandler</b>
		</field-exclude>
		<field-exclude>
			<a>$JAVASSIST_READ_WRITE_HANDLER</a>
			<b>$JAVASSIST_READ_WRITE_HANDLER</b>
		</field-exclude>
		<field-exclude>
			<a>properties</a>
			<b>properties</b>
		</field-exclude>
		<field copy-by-reference="true">
			<a>id</a>
			<b>id</b>
		</field>
		<field>
			<a>definitionId</a>
			<b>identifier</b>
		</field>
		<field>
			<a>definitionRevision</a>
			<b>revision</b>
		</field>
		<field>
			<a>definitionPath</a>
			<b>path</b>
		</field>
	</mapping>

	<mapping>
		<class-a>com.sirma.itt.seip.model.LinkSourceId</class-a>
		<class-b>com.sirma.itt.seip.model.LinkSourceId</class-b>

		<field-exclude>
			<a>operation</a>
			<b>operation</b>
		</field-exclude>
		<field-exclude>
			<a>fieldHandler</a>
			<b>fieldHandler</b>
		</field-exclude>
		<field-exclude>
			<a>$JAVASSIST_READ_WRITE_HANDLER</a>
			<b>$JAVASSIST_READ_WRITE_HANDLER</b>
		</field-exclude>

		<field>
			<a>id</a>
			<b>id</b>
		</field>
		<field copy-by-reference="true">
			<a>referenceType</a>
			<b>referenceType</b>
 		</field>
		<field copy-by-reference="true">
			<a>type</a>
			<b>type</b>
		</field>
		<field-exclude>
			<a>instance</a>
			<b>instance</b>
		</field-exclude>
	</mapping>
</mappings>