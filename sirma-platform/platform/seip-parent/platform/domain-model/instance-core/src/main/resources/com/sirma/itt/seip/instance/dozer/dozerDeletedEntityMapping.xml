<?xml version="1.0" encoding="UTF-8"?>
<mappings xmlns="http://dozer.sourceforge.net" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://dozer.sourceforge.net
          http://dozer.sourceforge.net/schema/beanmapping.xsd">

	<mapping>
		<class-a>com.sirma.itt.seip.instance.archive.ArchivedEntity</class-a>
		<class-b>com.sirma.itt.seip.domain.instance.ArchivedInstance</class-b>

		<field-exclude>
			<a>operation</a>
			<b>operation</b>
		</field-exclude>
		<field-exclude>
			<a>fieldHandler</a>
			<b>fieldHandler</b>
		</field-exclude>
		<field-exclude>
			<a>$JAVASSIST_READ_WRITE_HANDLER</a>
			<b>$JAVASSIST_READ_WRITE_HANDLER</b>
		</field-exclude>

		<field-exclude>
			<a>properties</a>
			<b>properties</b>
		</field-exclude>

		<field copy-by-reference="true">
			<a>id</a>
			<b>id</b>
		</field>

		<field>
			<a>definitionId</a>
			<b>identifier</b>
		</field>

		<field>
			<a>definitionRevision</a>
			<b>revision</b>
		</field>

		<field>
			<a>transactionId</a>
			<b>transactionId</b>
		</field>

		<field>
			<a>deletedOn</a>
			<b>deletedOn</b>
		</field>

		<field copy-by-reference="true">
			<a>targetId</a>
			<b>targetId</b>
		</field>

		<field>
			<a>majorVersion</a>
			<b>majorVersion</b>
		</field>

		<field>
			<a>minorVersion</a>
			<b>minorVersion</b>
		</field>

		<field>
			<a>createdOn</a>
			<b>createdOn</b>
		</field>

	</mapping>

</mappings>