PREFIX emf: <http://ittruse.ittbg.com/ontology/enterpriseManagementFramework#>
PREFIX ptop: <http://www.ontotext.com/proton/protontop#>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX owl: <http://www.w3.org/2002/07/owl#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX dc: <http://purl.org/dc/elements/1.1/>
PREFIX skos: <http://www.w3.org/2004/02/skos/core#>

DELETE data {
    GRAPH <http://ittruse.ittbg.com/ontology/enterpriseManagementFramework> {
		emf:hasReporter rdfs:range ptop:Agent.
		emf:hasReporter rdfs:subPropertyOf emf:hasAgent.
		emf:hasReporter rdfs:domain ptop:Entity. 
		emf:isReporterOf rdfs:range ptop:Entity. 
		emf:isReporterOf rdfs:subPropertyOf emf:isAgentOf. 
		emf:isReporterOf rdfs:domain ptop:Agent. 		
} 
}