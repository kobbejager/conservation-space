#################################################################
#    Object Properties
#################################################################

###  http://www.sirma.com/ontologies/2016/02/culturalHeritageConservation/YUL#assignedConsAct
yul:assignedConsAct rdf:type owl:ObjectProperty ;
                    owl:inverseOf yul:isAssignedConsActOf ;
                    emf:auditEvent "+addAssignedConservationAction|-removeAssignedConservationAction|changeAssignedConservationAction" ;
                    emf:isSearchable "true"^^xsd:boolean ;
                    dc:creator "Malvina Makarieva" ;
                    rdfs:label "Conservation activity"@en ;
                    skos:definition "All activities."@en .


###  http://www.sirma.com/ontologies/2016/02/culturalHeritageConservation/YUL#associatedWith
yul:associatedWith rdf:type owl:ObjectProperty ;
                   rdfs:range chc:CulturalObject ;
                   emf:auditEvent "+addAssociatedWith|-removeAssociatedWith|changeAssociatedWith" ;
                   emf:isSearchable "true"^^xsd:boolean ;
                   dc:creator "Malvina Makarieva" ;
                   rdfs:label "Associated with"@en ;
                   skos:definition "The Cultural Object associated with the current Cultural Object."@en .


###  http://www.sirma.com/ontologies/2016/02/culturalHeritageConservation/YUL#checkedBy
yul:checkedBy rdf:type owl:ObjectProperty ;
              rdfs:range emf:User ;
              emf:auditEvent "+addCheckedBy|-removeCheckedBy|changeCheckedBy" ;
              emf:isSearchable "true"^^xsd:boolean ;
              dc:creator "Malvina Makarieva" ;
              sec:autoAssignParentPermissionRole "COLLABORATOR" ;
              sec:autoAssignPermissionRole "COLLABORATOR" ;
              rdfs:label "Checked by"@en ;
              skos:definition "The person who has checked the object."@en .


###  http://www.sirma.com/ontologies/2016/02/culturalHeritageConservation/YUL#collectedBy
yul:collectedBy rdf:type owl:ObjectProperty ;
                rdfs:range emf:User ;
                emf:auditEvent "+addCollectedBy|-removeCollectedBy|changeCollectedBy" ;
                emf:isSearchable "true"^^xsd:boolean ;
                dc:creator "Malvina Makarieva" ;
                sec:autoAssignParentPermissionRole "COLLABORATOR" ;
                sec:autoAssignPermissionRole "COLLABORATOR" ;
                rdfs:label "Collected by"@en ;
                skos:definition "Name of individual who collected the sample."@en .


###  http://www.sirma.com/ontologies/2016/02/culturalHeritageConservation/YUL#isAssignedConsActOf
yul:isAssignedConsActOf rdf:type owl:ObjectProperty ;
                        rdfs:range emf:Document ;
                        emf:auditEvent "+addIsAassignedConservationActionOf|-removeIsAassignedConservationActionOf|changeIsAassignedConservationActionOf" ;
                        emf:isSearchable "true"^^xsd:boolean ;
                        dc:creator "Malvina Makarieva" ;
                        rdfs:label "Assigned action"@en ;
                        skos:definition "Processes actvity."@en .


###  http://www.sirma.com/ontologies/2016/02/culturalHeritageConservation/YUL#packedBy
yul:packedBy rdf:type owl:ObjectProperty ;
             rdfs:range emf:User ;
             emf:auditEvent "+addPackedBy|-removePackedBy|changePackedBy" ;
             emf:isSearchable "true"^^xsd:boolean ;
             dc:creator "Malvina Makarieva" ;
             rdfs:label "Packed by"@en ;
             skos:definition "The person who has packed the object."@en .


###  http://www.sirma.com/ontologies/2016/02/culturalHeritageConservation/YUL#requestedBy
yul:requestedBy rdf:type owl:ObjectProperty ;
                rdfs:range emf:User ;
                emf:auditEvent "+addRequestedBy|-removeRequestedBy|changeRequestedBy" ;
                emf:isSearchable "true"^^xsd:boolean ;
                dc:creator "Malvina Makarieva" ;
                rdfs:label "Requested by"@en ;
                skos:definition "Staff who is managing the outsourced work; primary contact."@en .


###  http://www.sirma.com/ontologies/2016/02/culturalHeritageConservation/YUL#reviewedBy
yul:reviewedBy rdf:type owl:ObjectProperty ;
               rdfs:range emf:User ;
               emf:auditEvent "+addReviewedBy|-removeReviewedBy|changeReviewedBy" ;
               emf:isSearchable "true"^^xsd:boolean ;
               dc:creator "Malvina Makarieva" ;
               sec:autoAssignParentPermissionRole "COLLABORATOR" ;
               sec:autoAssignPermissionRole "COLLABORATOR" ;
               rdfs:label "Review by"@en ;
               skos:definition "Identifies the person, responsible for the review of the cultural object."@en .


