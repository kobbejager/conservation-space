<http://www.sirma.com/ontologies/2016/02/culturalHeritageConservation> rdf:type owl:Ontology ;
                                                                        owl:imports <http://ittruse.ittbg.com/ontology/enterpriseManagementFramework> ,
                                                                                    <http://www.sirma.com/ontologies/2014/11/security> ;
                                                                        rdfs:label "Cultural Heritage Conservation Ontology Model"@en ;
                                                                        owl:versionInfo "0.1"@en ;
                                                                        rdfs:comment "Cultural Heritage Conservation Ontology Model used for Conservation Space project"@en ;
                                                                        dc:rights "What rights?"@en ;
                                                                        dc:subject "Cultural Heritage Conservation Ontology Model"@en ;
                                                                        dc:creator "Sirm ITT (Kiril Penev, Lidiya Dimitrova)"@en .

#################################################################
#    Object Properties
#################################################################

###  http://www.sirma.com/ontologies/2016/02/culturalHeritageConservation#approvedBy
chc:approvedBy rdf:type owl:ObjectProperty ;
               owl:inverseOf chc:hasApproved ;
               rdfs:domain ptop:Entity ;
               emf:auditEvent "+addApprovedBy|-removeApprovedBy|changeApprovedBy" ;
               dc:creator "Lidiya Dimitrova" ;
               rdfs:label "er godkendt af"@da ,
                          "is approved by"@en ;
               skos:definition "Defines person who has approved the document."@en .


###  http://www.sirma.com/ontologies/2016/02/culturalHeritageConservation#assignedPermissions
chc:assignedPermissions rdf:type owl:ObjectProperty ;
                        owl:inverseOf chc:hasAssignedPermissionsTo ;
                        dc:creator "Malvina Makarieva"@en ;
                        rdfs:label "Assigned permissions"@en ,
                                   "Tildelte tilladelser"@da ;
                        skos:definition "Assigned permissions."@en .


###  http://www.sirma.com/ontologies/2016/02/culturalHeritageConservation#attachmentOf
chc:attachmentOf rdf:type owl:ObjectProperty ;
                 owl:inverseOf chc:hasAttachment ;
                 rdfs:domain ptop:Entity ;
                 emf:auditEvent "+addAttachmentOf|-removeAttachmentOf|changeAttachmentOf" ;
                 dc:creator "Lidiya Dimitrova" ;
                 rdfs:label "er vedhæftning af"@da ,
                            "is attachment of"@en ;
                 skos:definition "Specifies of which object the current one is an attachment."@en .


###  http://www.sirma.com/ontologies/2016/02/culturalHeritageConservation#borrowedBy
chc:borrowedBy rdf:type owl:ObjectProperty ;
               owl:inverseOf chc:isBorrowerOf ;
               rdfs:domain chc:CulturalObject ;
               emf:auditEvent "+addBorrowedBy|-removeBorrowedBy|changeBorrowedBy" ;
               dc:creator "Lidiya Dimitrova" ;
               rdfs:label "er lånt af"@da ,
                          "is borrowed by"@en ;
               skos:definition "Indicates the institution that borrows the cultural object."@en .


###  http://www.sirma.com/ontologies/2016/02/culturalHeritageConservation#depicts
chc:depicts rdf:type owl:ObjectProperty ;
            owl:inverseOf chc:hasDepiction ;
            rdfs:domain emf:Image ;
            emf:auditEvent "+addDepicts|-removeDepicts|changeDepicts" ;
            dc:creator "Lidiya Dimitrova" ;
            rdfs:label "afbilder"@da ,
                       "depicts"@en ;
            skos:definition "Indicates the cultural object is depicted with image."@en .


###  http://www.sirma.com/ontologies/2016/02/culturalHeritageConservation#documents
chc:documents rdf:type owl:ObjectProperty ;
              owl:inverseOf chc:isDocumentedIn ;
              emf:auditEvent "+addDocuments|-removeDocuments|changeDocuments" ;
              dc:creator "Lidiya Dimitrova" ;
              rdfs:label "documents an object"@en ,
                         "dokumenterer en genstand"@da ;
              skos:definition "Indicates that the document is related to cultural object/sample."@en .


###  http://www.sirma.com/ontologies/2016/02/culturalHeritageConservation#duplicates
chc:duplicates rdf:type owl:ObjectProperty ,
                        owl:SymmetricProperty ;
               rdfs:domain chc:CulturalObject ;
               emf:auditEvent "+addDuplicates|-removeDuplicates|changeDuplicates" ;
               dc:creator "Lidiya Dimitrova" ;
               rdfs:label "duplicates"@en ,
                          "duplikerer"@da ;
               skos:definition "Indicates that the cultural object duplicates another cultural object."@en .


###  http://www.sirma.com/ontologies/2016/02/culturalHeritageConservation#exhibitedAt
chc:exhibitedAt rdf:type owl:ObjectProperty ;
                owl:inverseOf chc:hasCulturalObjectExhibited ;
                rdfs:domain chc:CulturalObject ;
                emf:auditEvent "+addToExhibition|-removeFromExhibition|changeExhibition" ;
                dc:creator "Lidiya Dimitrova" ;
                rdfs:label "exhibited on"@en ,
                           "udstillet på"@da ;
                skos:definition "The exhibition that the cultural object is part of."@en .


###  http://www.sirma.com/ontologies/2016/02/culturalHeritageConservation#hasApproved
chc:hasApproved rdf:type owl:ObjectProperty ;
                rdfs:domain ptop:Agent ;
                emf:auditEvent "+addHasApproved|-removeHasApproved|changeHasApproved" ;
                dc:creator "Lidiya Dimitrova" ;
                rdfs:label "har godkendt"@da ,
                           "has approved"@en ;
                skos:definition "Defines person who has approved the document."@en .


###  http://www.sirma.com/ontologies/2016/02/culturalHeritageConservation#hasAssignedConservator
chc:hasAssignedConservator rdf:type owl:ObjectProperty ;
                           rdfs:subPropertyOf owl:topObjectProperty ;
                           owl:inverseOf chc:isAssignedConservator ;
                           rdfs:domain ptop:Entity ;
                           emf:auditEvent "+addHasAssignedConservator|-removeHasAssignedConservator|changeHasAssignedConservator" ;
                           dc:creator "Malvina Makarieva" ;
                           rdfs:label "har tilknyttet konservator"@da ,
                                      "has assigned conservator"@en ;
                           skos:definition "Indicates the conservator assigned to the record."@en .


###  http://www.sirma.com/ontologies/2016/02/culturalHeritageConservation#hasAssignedPermissionsTo
chc:hasAssignedPermissionsTo rdf:type owl:ObjectProperty ;
                             rdfs:domain ptop:Agent ;
                             dc:creator "Malvina Makarieva"@en ;
                             rdfs:label "har tildelt tilladelser til"@da ,
                                        "has assigned permissions to"@en ;
                             skos:definition "Has assigned permissions on document."@en .


###  http://www.sirma.com/ontologies/2016/02/culturalHeritageConservation#hasAttachment
chc:hasAttachment rdf:type owl:ObjectProperty ;
                  rdfs:domain ptop:Entity ;
                  emf:auditEvent "+addHasAttachment|-removeHasAttachment|changeHasAttachment" ;
                  dc:creator "Lidiya Dimitrova" ;
                  rdfs:label "har vedhæftning"@da ,
                             "has attachment"@en ;
                  skos:definition "Specifies the attachments of the current object."@en .


###  http://www.sirma.com/ontologies/2016/02/culturalHeritageConservation#hasAuthor
chc:hasAuthor rdf:type owl:ObjectProperty ;
              owl:inverseOf chc:isAuthorOf ;
              rdfs:domain emf:Document ;
              emf:auditEvent "+addHasAuthor|-removeHasAuthor|changeHasAuthor" ;
              dc:creator "Lidiya Dimitrova" ;
              rdfs:label "har forfatter"@da ,
                         "has author"@en ;
              skos:definition "Indicates that the document has an author."@en .


###  http://www.sirma.com/ontologies/2016/02/culturalHeritageConservation#hasCoauthor
chc:hasCoauthor rdf:type owl:ObjectProperty ;
                owl:inverseOf chc:isCoauthorOf ;
                rdfs:domain emf:Document ;
                emf:auditEvent "+addHasCoauthor|-removeHasCoauthor|changeHasCoauthor" ;
                dc:creator "Lidiya Dimitrova" ;
                rdfs:label "har medforfatter"@da ,
                           "has coauthor"@en ;
                skos:definition "Indicates that the document has coauthor."@en .


###  http://www.sirma.com/ontologies/2016/02/culturalHeritageConservation#hasCulturalObjectExhibited
chc:hasCulturalObjectExhibited rdf:type owl:ObjectProperty ;
                               rdfs:domain chc:Exhibition ;
                               emf:auditEvent "+addHasCulturalObjectExhibited|-removeHasCulturalObjectExhibited|changeHasCulturalObjectExhibited" ;
                               dc:creator "Lidiya Dimitrova" ;
                               rdfs:label "exhibited cultural objects"@en ,
                                          "udstillede værker"@en ;
                               skos:definition "Indicates the cultural objects being part of the exhibition."@en .


###  http://www.sirma.com/ontologies/2016/02/culturalHeritageConservation#hasDepiction
chc:hasDepiction rdf:type owl:ObjectProperty ;
                 rdfs:domain ptop:Entity ;
                 emf:auditEvent "+addHasDepiction|-removeHasDepiction|changeHasDepiction" ;
                 dc:creator "Lidiya Dimitrova" ;
                 rdfs:label "har afbildning"@da ,
                            "has depiction"@en ;
                 skos:definition "Indicates the cultural object is depicted with image."@en .


###  http://www.sirma.com/ontologies/2016/02/culturalHeritageConservation#hasSample
chc:hasSample rdf:type owl:ObjectProperty ;
              owl:inverseOf chc:isTakenFrom ;
              rdfs:domain chc:CulturalObject ;
              emf:auditEvent "+addHasSample|-removeHasSample|changeHasSample" ;
              dc:creator "Lidiya Dimitrova" ;
              rdfs:label "har prøve"@da ,
                         "has sample"@en ;
              skos:definition "Indicates the sample which is taken from the cultural object."@en .


###  http://www.sirma.com/ontologies/2016/02/culturalHeritageConservation#hasSupervisor
chc:hasSupervisor rdf:type owl:ObjectProperty ;
                  owl:inverseOf chc:isSupervisorOf ;
                  rdfs:domain ptop:Entity ;
                  emf:auditEvent "+addHasSupervisor|-removeHasSupervisor|changeHasSupervisor" ;
                  dc:creator "Lidiya Dimitrova"@en ;
                  rdfs:label "har leder"@da ,
                             "has supervisor"@en ;
                  skos:definition "Indicates the document/activity is supervised by supervisor"@en .


###  http://www.sirma.com/ontologies/2016/02/culturalHeritageConservation#isAssignedConservator
chc:isAssignedConservator rdf:type owl:ObjectProperty ;
                          rdfs:domain ptop:Agent ;
                          emf:auditEvent "+addIsAssignedConservator|-removeIsAssignedConservator|changeIsAssignedConservator" ;
                          dc:creator "Malvina Makarieva" ;
                          rdfs:label "er tilknyttet konservator til"@da ,
                                     "is assigned conservator of"@en ;
                          skos:definition "The record that the user is assigned conservator."@en .


###  http://www.sirma.com/ontologies/2016/02/culturalHeritageConservation#isAuthorOf
chc:isAuthorOf rdf:type owl:ObjectProperty ;
               rdfs:domain ptop:Agent ;
               emf:auditEvent "+addIsAuthorOf|-removeIsAuthorOf|changeIsAuthorOf" ;
               dc:creator "Lidiya Dimitrova"@en ;
               rdfs:label "er forfatter af"@da ,
                          "is author of"@en ;
               skos:definition "Indicates who is the author of the document."@en .


###  http://www.sirma.com/ontologies/2016/02/culturalHeritageConservation#isBorrowerOf
chc:isBorrowerOf rdf:type owl:ObjectProperty ;
