###  http://www.sirma.com/ontologies/2016/02/culturalHeritageConservation#isSupervisorOf
chc:isSupervisorOf rdf:type owl:ObjectProperty ;
                   rdfs:domain ptop:Agent ;
                   emf:auditEvent "+addIsSupervisorOf|-removeIsSupervisorOf|changeIsSupervisorOf" ;
                   dc:creator "Lidiya Dimitrova"@en ;
                   rdfs:label "er leder af"@da ,
                              "is supervisor of"@en ;
                   skos:definition "Indicates the supervisor of the document/activity."@en .


###  http://www.sirma.com/ontologies/2016/02/culturalHeritageConservation#isTakenFrom
chc:isTakenFrom rdf:type owl:ObjectProperty ;
                rdfs:domain chc:Sample ;
                emf:auditEvent "+addIsTakenFrom|-removeIsTakenFrom|changeIsTakenFrom" ;
                dc:creator "Lidiya Dimitrova" ;
                rdfs:label "er taget fra"@da ,
                           "is taken from"@en ;
                skos:definition "Indicates the cultural object that the sample is taken from."@en .


###  http://www.sirma.com/ontologies/2016/02/culturalHeritageConservation#lentBy
chc:lentBy rdf:type owl:ObjectProperty ;
