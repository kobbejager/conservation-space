          dc:creator "Lidiya Dimitrova"@en ;
          sec:allowInheritLibraryPermissions "true"^^xsd:boolean ;
          sec:allowInheritParentPermissions "false"^^xsd:boolean ;
          rdfs:label "Frame"@en ,
                     "Ramme"@da ;
          skos:definition "A frame is a decorative molding that typically is attached to a painting or other cultural object especially when that cultural object is displayed. It typically surrounds and covers the edges of the cultural object. It is typically made of wood/plaster/gold leaf, but can be any material."@en .


###  http://www.sirma.com/ontologies/2016/02/culturalHeritageConservation/SMK#Installation
smk:Installation rdf:type owl:Class ;
                 rdfs:subClassOf chc:CulturalObject ;
                 emf:classCategory "culturalobject" ;
