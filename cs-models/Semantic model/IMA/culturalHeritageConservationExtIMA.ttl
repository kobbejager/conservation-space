###  http://www.sirma.com/ontologies/2016/02/culturalHeritageConservation/IMA#checkedBy
ima:checkedBy rdf:type owl:ObjectProperty ;
              owl:inverseOf ima:hasChecked ;
              rdfs:domain chc:CulturalObject ;
              emf:auditEvent "+addCheckedBy|-removeCheckedBy|changeCheckedBy" ;
              dc:creator "Anna Radeva" ;
              rdfs:label "is checked by"@en ;
              skos:definition "Checked by"@en .


