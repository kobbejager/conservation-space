###  http://www.sirma.com/ontologies/2016/02/culturalHeritageConservation/IMA#WeaponsAndAmmunition
ima:WeaponsAndAmmunition rdf:type owl:Class ;
                         rdfs:subClassOf chc:CulturalObject ;
                         emf:classCategory "culturalobject" ;
                         emf:definitionId "objectinstance" ;
                         emf:isCreateable "true"^^xsd:boolean ;
                         emf:isPartOfObjectLibrary "true"^^xsd:boolean ;
                         emf:isSearchable "true"^^xsd:boolean ;
                         dc:creator "Lidiya Dimitrova"@en ;
                         sec:allowInheritLibraryPermissions "true"^^xsd:boolean ;
                         sec:allowInheritParentPermissions "false"^^xsd:boolean ;
                         rdfs:label "Weapons and Ammunition"@en ;
                         skos:definition "This record type contains descriptors for implements and mechanisms designed to be used as a means of physical attack or defense. Included are hand-held weapons such as \"swords\", components of weapons (e.g., \"arrowheads\"), and objects propelled by firearms (e.g., \"cartridges\"). Also included are descriptors for hunting weapons (e.g., \"boar spears\") and for objects derived from weapons but used for purely ceremonial purposes (e.g., \"dress swords\"). The Curatorial Department will determine whether it is Ethnographic, Native American, Asian, etc."@en .


