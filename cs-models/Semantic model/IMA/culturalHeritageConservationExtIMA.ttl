ima:CostumeOrCostumeAccessories rdf:type owl:Class ;
                                rdfs:subClassOf chc:CulturalObject ;
                                emf:classCategory "culturalobject" ;
                                emf:definitionId "objectinstance" ;
                                emf:isCreateable "true"^^xsd:boolean ;
                                emf:isPartOfObjectLibrary "true"^^xsd:boolean ;
                                emf:isSearchable "true"^^xsd:boolean ;
                                dc:creator "Lidiya Dimitrova"@en ;
                                sec:allowInheritLibraryPermissions "true"^^xsd:boolean ;
                                sec:allowInheritParentPermissions "false"^^xsd:boolean ;
                                rdfs:label "Costume or Costume Accessories"@en ;
                                skos:definition "This record type contains descriptors for objects worn or carried for warmth, protection, embellishment, or for symbolic purposes. It includes descriptors for garments considered as the main item of dress (e.g., \"shirts,\" \"trousers\"), descriptors for garments worn under the main garments (e.g., \"undershirts\"), and descriptors for garments worn over the main garments (e.g., \"parkas\").  Also included are descriptors for protective wear, including types of armor; vestments and other ceremonial garments; uniforms. "@en .


