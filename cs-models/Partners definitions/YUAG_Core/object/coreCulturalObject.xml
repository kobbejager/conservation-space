						<control-param id="range" name="range">ptop:Organization,chc:Person</control-param>
						<control-param id="restrictions" name="restrictions">{"field": "emf:status", "operator": "in", "type": "codeList", "value": ["ACTIVE"]}</control-param>
					</control>
				</field>	
				<field name="yuag:previousLocation" type="an..10000" displayType="editable" order="199" uri="yuag:previousLocation" >
					<control id="RICHTEXT"/>
				</field>	
				<field name="chcart:currentLocation" type="an..10000" displayType="editable" order="200" uri="chcart:currentLocation" >
					<control id="RICHTEXT"/>
				</field>	
