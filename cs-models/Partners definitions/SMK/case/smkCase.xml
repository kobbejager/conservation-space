						<control-param type="default_value_pattern" id="template" name="template">$[emf:createdBy.department]</control-param>
					</control>	
				</field>
				<field name="activityType" type="an..180" mandatory="true" displayType="editable" codelist="CL239" label="smk.activityType.label" tooltip="smk.activityType.tooltip" uri="chc:activityType" />
				<field name="title" type="an..1000" mandatory="true" displayType="editable" label="smk.title.label" tooltip="smk.title.tooltip" uri="dcterms:title" />
				<field name="description" type="an..1024" displayType="editable" label="smk.description.label" tooltip="smk.description.tooltip" previewEmpty="false" uri="dcterms:description" />
