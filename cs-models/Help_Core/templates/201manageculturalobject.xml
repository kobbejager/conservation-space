        </field>
        <field name="content" type="ANY" multiValued="false" mandatory="false" displayType="system" previewEmpty="true" dmsType="FORBIDDEN" uri="FORBIDDEN">
            <value>&lt;div data-tabs-counter="4"&gt; 
 &lt;section data-id="SkRrp0jJQ" data-title="Help" data-default="true" data-show-navigation="false" data-show-comments="false" data-revision="exportable" data-locked="false" data-user-defined="false"&gt; 
